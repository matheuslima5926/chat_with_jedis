package DAO;

import entity.User;
import redis.clients.jedis.Jedis;

public class UserDAO {
	private Jedis dao = JedisDAO.connectJedis("localhost");
	public static boolean saveUser(User user){
		return false;
	}
	public static User findUserByNick(User user){
		
		if(dao.hget("user", "nickname")!= null){
			return user;
		}
		return null;
	}
}
